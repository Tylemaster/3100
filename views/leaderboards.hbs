<div id="leaderboards">

<table id = "leaderTable" class="table">
    <tr class="table">
        <th onclick="sortTable(0)" class="table"> User </th>
        <th onclick="sortTable(1)" class="table"> Highscore </th>
    </tr>
    {{#each users}}
    <tr class="table"> 
        <td class="table"> {{this.username}}</td>
        <td class="table"> {{this.highscore}}</td>
    </tr>
    {{/each}}
</table>
<script src='/leaderboards.js'></script>

</div>
